---
// src/components/SwiperCarouselWrapper.astro

import SwiperCarousel from "./blog/SwiperCarousel.jsxl.jsx";

interface Article {
  title: string;
  description?: string;
  href: string;
  src?: string;
  date: Date;
  authors: {
    name: string;
    profileIcon: string;
  }[];
}

const { articles } = Astro.props;
---

<SwiperCarousel client:only="react" articles={articles} />
